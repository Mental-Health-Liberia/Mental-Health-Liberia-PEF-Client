{
  "tabs": [
    {
      "name": "basic",
      "title": "Basic Configuration",
      "fieldsets": [
        {
          "title": "Patient",
          "elements": [
            {
              "name": "patient_id",
              "title": "Patient ID",
              "type": "patient_id"
            },
            {
              "name": "referral_source",
              "title": "Referral Source",
              "type": "select",
              "options": [
                "Clinician",
                "Messages",
                "Other",
                "No response"
              ]
            }
          ]
        },
        {
          "title": "Clinician",
          "elements": [
            {
              "name": "clinician_id",
              "title": "Clinician ID",
              "type": "text",
              "rules": {
                "number": true,
                "required": true
              }
            },
            {
              "name": "clinician_training",
              "title": "Clinician Training Level",
              "type": "select",
              "options": [
                "No formal training",
                "Community health worker training",
                "Physician's assistant training",
                "General clinician training",
                "Psychchiatric clinician training",
                "Other",
                "No response"
              ]
            }
          ]
        },
        {
          "title": "Visit",
          "elements": [
            {
              "name": "service_location",
              "title": "Location of Service",
              "type": "text",
              "placeholder": "Clinic or Hospital Name"
            },
            {
              "name": "date",
              "title": "Date of Service",
              "type": "datepicker"
            },
            {
              "name": "time",
              "title": "Time of Service",
              "type": "timepicker"
            },
            {
              "name": "reason",
              "title": "Reason for Visit",
              "type": "radio",
              "options": [
                "Seeking Mental Health Care",
                "Other"
              ]
            }
          ]
        }
      ]
    },
    {
      "name": "demographics",
      "title": "Demographics",
      "fieldsets": [
        {
          "title": "Patient",
          "elements": [
            {
              "name": "age",
              "title": "Age (years)",
              "type": "text",
              "rules": {
                "number": true,
                "required": true
              }
            },
            {
              "name": "life_role_days",
              "title": "Number of Days in Life Role",
              "type": "text",
              "rules": {
                "number": true,
                "required": true
              }
            },
            {
              "name": "distance_travelled_to_clinic",
              "title": "Distance Travelled to Clinic (kilometers)",
              "type": "text",
              "rules": {
                "number": true,
                "required": true
              }
            },
            {
              "name": "country_of_residence",
              "title": "Country of Residence",
              "type": "select",
              "options": [
                "Bomi",
                "Bong",
                "Gbarpolo",
                "Grand Bassa",
                "Grand Cape Mount",
                "Grand Gedah",
                "Grand Kru",
                "Lofa",
                "Margibi",
                "Maryland",
                "Montserrado",
                "Nimba",
                "River Cess",
                "River Gee",
                "Sinoe",
                "Other"
              ]
            },
            {
              "name": "marital_status",
              "title": "Marital Status",
              "type": "select",
              "options": [
                "Single / Never Married",
                "Married",
                "Divorced",
                "Windowed",
                "No Response"
              ]
            },
            {
              "name": "education",
              "title": "Education",
              "type": "select",
              "options": [
                "No Formal Education",
                "Grade / Primary School",
                "High School",
                "Some University",
                "University",
                "No Response"
              ]
            }
          ]
        }
      ]
    },
    {
      "name": "symptoms_and_functioning",
      "title": "Symptoms and Functioning",
      "fieldsets": [
        {
          "title": "Test Results",
          "elements": [
            {
              "name": "phq9",
              "title": "PHQ-9 Score",
              "type": "text",
              "rules": {
                "number": true,
                "required": true
              }
            },
            {
              "name": "gaf",
              "title": "GAF Score",
              "type": "text",
              "rules": {
                "number": true,
                "required": true
              }
            },
            {
              "name": "cage",
              "title": "CAGE Score",
              "type": "text",
              "rules": {
                "number": true,
                "required": true
              }
            },
            {
              "name": "who-das",
              "title": "WHO-DAS Score",
              "type": "text",
              "rules": {
                "number": true,
                "required": true
              }
            },
            {
              "name": "euroqol",
              "title": "Euroqol Score",
              "type": "text",
              "rules": {
                "number": true,
                "required": true
              }
            }
          ]
        },
        {
          "title": "Other Results",
          "elements": [
            {
              "name": "other_symptoms",
              "title": "Other Symptoms",
              "type": "text"
            }
          ]
        }
      ]
    },
    {
      "name": "diagnosis",
      "title": "Diagnosis",
      "fieldsets": [
        {
          "title": "Mood Disorder",
          "elements": [
            {
              "name": "primary",
              "title": "Primary",
              "type": "select",
              "options": [
                "Major Depressive Disorder (Recurrent)",
                "Bipolar Disorder - Most Recent Episode Depressed",
                "Bipolar Disorder - Most Recent Episode Manic",
                "Bipolar Disorder - Most Recent Episode Mixed",
                "Mood Disorder NOS",
                "Mood Disorder due to Medical Condition",
                "None"
              ]
            },
            {
              "name": "secondary",
              "title": "Secondary",
              "type": "select",
              "options": [
                "Full Remission",
                "Partial Remission",
                "Mild",
                "Moderate",
                "Severe without Psychotic Features",
                "Severe with Psychotic Features",
                "None"
              ]
            }
          ]
        },
        {
          "title": "Other Disorders",
          "elements": [
            {
              "name": "anxiety",
              "title": "Anxiety",
              "type": "select",
              "options": [
                "Generalized anxiety disorder",
                "Panic disorder with agoraphobia",
                "Panic disorder without agoraphobia",
                "Agoraphobia without history of panic disorder",
                "Specific phobia",
                "Social phobia",
                "Obsessive compulsive disorder",
                "Post-traumatic stress disorder",
                "Acute stress",
                "Anxiety disorder due to general medical condition",
                "Anxiety disorder due to specific medical condition",
                "Anxiety disoder due to NOS",
                "None"
              ]
            },
            {
              "name": "psychotic",
              "title": "Psychotic",
              "type": "select",
              "options": [
                "Schizophrenia catatonic type",
                "Schizophrenia disorganized type",
                "Schizophrenia paranoid type",
                "Schizophrenia residual type",
                "Schizophrenia undifferentiated type",
                "Schizoaffective disorder",
                "Psychotic disorder due to medical condition with delusions",
                "Psychotic disorder due to medical condition with hallucinations",
                "Psychotic disorder NOS",
                "None"
              ]
            },
            {
              "name": "somatoform",
              "title": "Somatoform",
              "type": "select",
              "options": [
                "Somatization disorder",
                "Undifferentiated somatoform disorder",
                "Conversion disorder",
                "Pain disorder with both psychological factors and general medical condition",
                "Pain disorder with psychological factors",
                "Hypochondriasis",
                "Body dysmorphic disorder",
                "Somatoform disorder NOS",
                "None"
              ]
            }
          ]
        },
        {
          "title": "Substance Abuse",
          "elements": [
            {
              "name": "first_level",
              "title": "First Level",
              "type": "select",
              "options": [
                "Alcohol-related disorder",
                "Cannabis",
                "Cocaine",
                "Nicotine dependence",
                "Opioid",
                "Sedative-, hypnotic-, or anxiolytic",
                "None"
              ]
            },
            {
              "name": "second_level",
              "title": "Second Level",
              "type": "select",
              "options": [
                "-induced mood disorder",
                "-induced psychotic disorder with delusions",
                "-induced psychotic disorder with hallucinations",
                "Intoxication",
                "Withdrawal",
                "None"
              ]
            }
          ]
        },
        {
          "title": "Epilepsy",
          "elements": [
            {
              "name": "seizure",
              "title": "Seizure",
              "type": "select",
              "options": [
                "With grand mal seizure",
                "With petit mal seizure",
                "Without seizure",
                "None"
              ]
            },
            {
              "name": "epilepsy_symptoms",
              "title": "Other Epilepsy Symptoms",
              "type": "text"
            },
            {
              "name": "seizure_count",
              "title": "Number of Seizures per Month",
              "type": "text",
              "rules": {
                "number": true
              }
            }
          ]
        },
        {
          "title": "Other",
          "elements": [
            {
              "name": "other_conditions",
              "title": "Other Medical Conditions",
              "type": "text"
            },
            {
              "name": "secondary_tertiary_diagnosis",
              "title": "Secondary / Tertiary Diagnosis",
              "type": "text"
            }
          ]
        }
      ]
    }
  ]
}